
@model IQueryable<Paraglider.sl.DTOs.PilotDto>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
 
    <form method="Post">
        <div class="form-group">
            <div class="input-group">
                <input type="search" class="form-control" name="name" value="" />
                <div class="input-group-append">
                    <button class="btn btn-secondary">
                        <i class="fas fa-search"></i>
                    </button>
                </div>

            </div>
        </div>
    </form>

    <table>
        <thead>
            <tr>
                <th>id </th>
                <th>first</th>
                <th>last</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var pilot in Model)
            {

            <tr>
                <th>@pilot.Id </th>
                <th>@pilot.FirstName</th>
                <th>@pilot.LastName</th>
            </tr>
            }
        </tbody>
    </table>
</body>
</html>